{
    "manifestVersion": 1,
    "id": "amazon-app-store-edit",
    "publisher": "MaxenceRaoux",
    "version": "0.1.6",
    "name": "Amazon AppStore create or edit update API",
    "description": "Create a new Edit or get the id of the existing Edit from the Amazon AppStore api and make continuous delivery on azure pipeline build or release.",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "categories": [
        "Azure Pipelines"
    ],
    "files": [
        {
            "path": "AmazonAppStoreEditTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "AmazonAppStoreEditTask"
            }
        }
    ]
}