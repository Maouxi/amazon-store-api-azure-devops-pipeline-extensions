{
    "manifestVersion": 1,
    "id": "amazon-app-store-auth",
    "publisher": "MaxenceRaoux",
    "version": "0.1.21",
    "name": "Amazon AppStore authentication API",
    "description": "Get an authentication access token for the Amazon AppStore api and make continuous delivery on azure pipeline build or release.",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "files": [
        {
            "path": "AmazonAppStoreAuthTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "AmazonAppStoreAuthTask"
            }
        }
    ]
}