{
    "manifestVersion": 1,
    "id": "amazon-app-store-replace-apk",
    "publisher": "MaxenceRaoux",
    "version": "0.2.2",
    "name": "Amazon AppStore replace apk API",
    "description": "Replace an apk of an active edit for the Amazon AppStore submission API and make continuous delivery on Azure DevOps Services pipeline build or release.",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/Maouxi/amazon-store-api-azure-devops-pipeline-extensions"
    },
    "tags": [
        "amazon",
        "appstore",
        "submission",
        "api",
        "replace",
        "apk"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "categories": [
        "Azure Pipelines"
    ],
    "screenshots": [
        {
            "path": "images/screenshot.png"
        }
    ],
    "files": [
        {
            "path": "AmazonAppStoreReplaceApkTask"
        },
        {
            "path": "images/screenshot.png",
            "addressable": true
        },
        {
            "path": "images/extension-icon.png",
            "addressable": true
        }
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/Maouxi/amazon-store-api-azure-devops-pipeline-extensions"
        },
        "support": {
            "uri": "https://github.com/Maouxi/amazon-store-api-azure-devops-pipeline-extensions"
        },
        "issues": {
            "uri": "https://github.com/Maouxi/amazon-store-api-azure-devops-pipeline-extensions/issues"
        }
    },
    "icons": {
        "default": "images/extension-icon.png"
    },
    "badges": [
        {
            "href": "https://dev.azure.com/maxenceraoux/AmazonDevOpsExtensions/_build?definitionId=7",
            "uri": "https://dev.azure.com/maxenceraoux/AmazonDevOpsExtensions/_apis/build/status/ReplaceApkTask%20build?branchName=develop",
            "description": "Develop"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "AmazonAppStoreReplaceApkTask"
            }
        }
    ]
}