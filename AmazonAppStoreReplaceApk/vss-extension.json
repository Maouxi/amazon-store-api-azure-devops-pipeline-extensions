{
    "manifestVersion": 1,
    "id": "amazon-app-store-replace-apk",
    "publisher": "MaxenceRaoux",
    "version": "0.1.0",
    "name": "Amazon AppStore replace apk API",
    "description": "Replace an apk of an active edit for the Amazon AppStore api and make continuous delivery on azure pipeline build or release.",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "categories": [
        "Azure Pipelines"
    ],
    "files": [
        {
            "path": "AmazonAppStoreReplaceApkTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "AmazonAppStoreReplaceApkTask"
            }
        }
    ]
}