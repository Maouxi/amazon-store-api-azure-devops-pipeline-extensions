{
    "manifestVersion": 1,
    "id": "amazon-app-store-prepare",
    "publisher": "MaxenceRaoux",
    "version": "0.1.1",
    "name": "Prepare call to Amazon AppStore API",
    "description": "Prepare call to amazon AppStore Api to make continuous delivery on azure pipeline build or release.",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "files": [
        {
            "path": "AmazonAppStorePrepareTask"
        }
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "AmazonAppStorePrepareTask"
            }
        }
    ]
}